<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="Rewrite all urls to index.html">
                    <match url=".*\.html|css/|images/|js/|data/|lib/|templates/|fonts/|favicon.ico" negate="true" />
                    <action type="Rewrite" url="/index.html" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
